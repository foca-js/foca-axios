{
  "name": "foca-axios",
  "version": "4.1.1",
  "repository": "git@github.com:foca-js/foca-axios.git",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "prepublishOnly": "tsup",
    "prepare": "husky",
    "deploy:main": "sh scripts/develop-deploy-main.sh",
    "sync:main": "sh scripts/develop-sync-main.sh"
  },
  "volta": {
    "node": "20.15.1",
    "pnpm": "9.5.0"
  },
  "packageManager": "pnpm@9.5.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "axios": "^1.7.7",
    "clone": "^2.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@release-it/conventional-changelog": "^9.0.1",
    "@types/clone": "^2.1.4",
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^2.1.3",
    "axios-mock-adapter": "^2.1.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "sleep-promise": "^9.1.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
